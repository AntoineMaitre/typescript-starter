    "e2e:watch": "jest --watch --config=e2e/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^4.5.9",
    "@nestjs/core": "^4.5.10",
    "@nestjs/microservices": "^4.5.8",
    "@nestjs/mongoose": "^2.0.0",
    "@nestjs/swagger": "^1.1.3",
    "@nestjs/testing": "^4.4.0",
    "@nestjs/websockets": "^4.4.0",
    "@types/mongoose": "^4.7.32",
    "config": "^1.29.2",
    "iridium": "^7.2.5",
    "jsonwebtoken": "^8.1.0",
    "log4js": "^2.4.1",
    "moment": "^2.20.1",
    "mongoose": "^4.13.9",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "@types/jest": "^22.0.1",
    "@types/node": "^8.0.28",
    "@types/supertest": "^2.0.4",
    "jest": "^22.1.3",
    "mockgoose": "^7.3.3",
    "mockgoose-fix": "^7.3.6",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "ts-jest": "^22.0.1",
    "ts-node": "^3.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
